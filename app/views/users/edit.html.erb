<h1>Update Account</h1>

<%= form_for @user, url: user_path(@user) do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username, placeholder: "#{@user.username}" %>

  <%= f.label :email %>
  <%= f.email_field :email, placeholder: "#{@user.email}" %>

  <%= f.submit "Update Account" %>
<% end %>
